tag:
type: txt
help: Name SIP profile
syntax:expression: pattern $VAR(@) "^[-_a-zA-Z0-9.]+$" ; "invalid Profile name \"$VAR(@)\""
create:expression: exec "sudo /opt/vyatta/sbin/vyatta-freeswitch.pl --profile=$VAR(@) --action=create"
commit:expression: exec "sudo /opt/vyatta/sbin/vyatta-freeswitch.pl --profile=$VAR(@) --action=update"
delete:expression: exec "sudo /opt/vyatta/sbin/vyatta-freeswitch.pl --profile=$VAR(@) --action=delete"
